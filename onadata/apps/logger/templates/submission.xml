{% if formid %}
<OpenRosaResponse xmlns="http://openrosa.org/http/response">
    <message>{{message}}</message>
    <submissionMetadata xmlns="http://www.opendatakit.org/xforms" id="{{formid}}" {% if version %} version="{{version}}"{% endif %}{% if encrypted %} encrypted="yes"{% endif %} instanceID="{{ instanceID }}" submissionDate="{{submissionDate}}" isComplete="true" markedAsCompleteDate="{{markedAsCompleteDate}}"/>
</OpenRosaResponse>{% endif %}
